{
  "plugins": ["cypress", "prettier"],
  "extends": [
    "eslint:recommended",
    "eslint-config-prettier",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-restricted-modules": "error"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
